INSERIR
INSERT INTO usuarios(nome, email, idade) VALUES(
    "Marilda", 
    "marilda@teste.com",
    58
);

ATUALIZAR
UPDATE usuarios SET nome = "Nome de Teste" WHERE nome = "Marcelino Neto";

DELETAR
DELETE FROM usuarios WHERE nome = "Marilda";

