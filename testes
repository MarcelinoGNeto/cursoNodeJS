COMANDOS SQL

SHOW DATABASES; //visualizar todas os bancos de dados
CREATE DATABASE nomeDoBancoDeDados; //criar novo Banco de Dados
CREATE TABLE nomeDaTabelausuarios(nome VARCHAR(50), email VARCHAR(100), idade INT); //Criar nova nomeDaTabela 
SELECT * FROM usuarios; //mostrar dados de uma Tabela

